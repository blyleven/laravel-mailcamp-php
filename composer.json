{
  "name": "voicecode/laravel-mailcamp",
  "type": "library",
  "description": "Laravel Mailcamp PHP library",
  "keywords": [
    "Voicecode", "Laravel", "Mailcamp", "PHP"
  ],
  "homepage": "https://voicecode.nl",
  "license": "MIT",
  "authors": [
    {
      "name": "Voicecode B.V.",
      "homepage": "https://voicecode.nl",
      "email": "info@voicecode.nl"
    }
  ],
  "require": {
    "php": "^7.3|^8.0"
  },
  "require-dev": {
    "symfony/dotenv": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Voicecode\\Mailcamp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Voicecode\\Mailcamp\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
        "providers": [
            "Voicecode\\Mailcamp\\MailcampServiceProvider"
        ],
        "aliases": {
            "Mailcamp": "Voicecode\\Mailcamp\\MailcampFacade"
        }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
